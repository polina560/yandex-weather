{
  "name": "peppers/yii-template",
  "version": "0.0.1",
  "description": "Yii 2 Advanced Project Template",
  "keywords": ["yii2", "vue", "framework", "advanced", "project template"],
  "type": "template",
  "license": "GPL-3.0-only",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Michail Kropuhinsky",
      "email": "m.kropukhinsky@peppers-studio.ru"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "bower-asset/bootswatch": "^5.2",
    "bower-asset/jquery-colorbox": "^1.6.4",
    "codemix/yii2-streamlog": "^1.3",
    "guzzlehttp/guzzle": "^7.8.0",
    "kartik-v/yii2-bootstrap5-dropdown": "^1.0",
    "kartik-v/yii2-date-range": "^1.7",
    "kartik-v/yii2-datecontrol": "^1.9",
    "kartik-v/yii2-editable": "^1.8.0",
    "kartik-v/yii2-export": "^1.4",
    "kartik-v/yii2-icons": "^1.4",
    "kartik-v/yii2-nav-x": "^1.2",
    "kartik-v/yii2-tabs-x": "^1.2",
    "kartik-v/yii2-validators": "^1.0",
    "kartik-v/yii2-widget-colorinput": "^1.0",
    "kartik-v/yii2-widget-datepicker": "^1.4",
    "kartik-v/yii2-widget-datetimepicker": "^1.5",
    "kartik-v/yii2-widget-fileinput": "^1.1",
    "kartik-v/yii2-widget-select2": "^2.2",
    "kartik-v/yii2-widget-sidenav": "^1.0",
    "kartik-v/yii2-widget-switchinput": "^1.3",
    "kartik-v/yii2-widget-timepicker": "^1.0",
    "league/flysystem": "^1.1",
    "league/flysystem-aws-s3-v3": "^1.0",
    "league/flysystem-cached-adapter": "^1.1",
    "monolog/monolog": "^2.9",
    "openspout/openspout": "^v4.23",
    "pimple/pimple": "^v3.5",
    "pug/yii2": "^1.2",
    "scssphp/scssphp": "^v1.12",
    "skeeks/yii2-imagine": "^2.3",
    "symfony/css-selector": "^v7.0",
    "symfony/dom-crawler": "^v7.0",
    "symfony/event-dispatcher": "^v7.0",
    "symfony/http-foundation": "^v7.0",
    "symfony/http-kernel": "v7.0",
    "symfony/mime": "^v7.0",
    "trntv/yii2-aceeditor": "^2.1",
    "vlucas/phpdotenv": "^v5.6",
    "yiisoft/yii2": "~2.0.40",
    "yiisoft/yii2-bootstrap5": "^2.0",
    "yiisoft/yii2-httpclient": "^2.0",
    "yiisoft/yii2-queue": "^2.3",
    "yiisoft/yii2-redis": "^2.0",
    "yiisoft/yii2-symfonymailer": "^2.0.0"
  },
  "require-dev": {
    "bower-asset/swagger-ui": "^4.0",
    "codeception/c3": "^2.9",
    "codeception/codeception": "^5.0",
    "codeception/module-asserts": "^3.0",
    "codeception/module-filesystem": "^3.0",
    "codeception/module-phpbrowser": "^3.0",
    "codeception/module-rest": "^3.3",
    "codeception/module-webdriver": "^4.0",
    "codeception/module-yii2": "^1.1",
    "codeception/verify": "^3.0",
    "doctrine/annotations": "^2.0",
    "phpunit/phpunit": "^10.0",
    "roave/security-advisories": "dev-latest",
    "yiisoft/yii2-debug": "^2.1",
    "yiisoft/yii2-faker": "^2.0",
    "yiisoft/yii2-gii": "^2.2",
    "zircote/swagger-php": "^4.7"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true,
      "nodejs-php-fallback/nodejs-php-fallback": true,
      "codeception/c3": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "CKSource\\": "CKSource/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "admin\\tests\\": ["admin/tests/", "admin/tests/_support/"],
      "api\\tests\\": ["api/tests/", "api/tests/_support/"],
      "common\\tests\\": ["common/tests/", "common/tests/_support/"],
      "frontend\\tests\\": ["frontend/tests/", "frontend/tests/_support/"]
    }
  }
}
